[receive]
	requireChangeId = true
[access "refs/*"]
	read = group TYPO3v4/Extensions/news
[access "refs/for/refs/*"]
	push = group TYPO3v4/Extensions/news
	pushMerge = group TYPO3v4/Extensions/news
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group TYPO3v4/Extensions/news
	label-Verified = -1..+2 group TYPO3v4/Extensions/news
	submit = group TYPO3v4/Extensions/news
[access "refs/tags/*"]
	pushTag = group TYPO3v4/Extensions/news
